
name = "hsrdb-api"
main = "src/entry.py"
compatibility_date = "2026-02-20"
compatibility_flags = ["python_workers"]

[[d1_databases]]
binding = "DB"
database_name = "hsrdb"
# Fill with the UUID from: wrangler d1 create hsrdb
database_id = " c848b7fc-405a-480b-8b85-aa091da341f0"

# Placeholder routes. Replace with your real zone/domain and /api/* path.
[[routes]]
pattern = "example.com/api/*"
zone_name = "example.com"
